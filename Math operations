import math

n = 2.3

print(math.ceil(n))

print(math.floor(n))

a = -10

print(math.fabs(a))

b = 5

print(math.factorial(b))

print(math.copysign(5.5,-10))

print(math.gcd(5,15))

print(math.exp(4))

print(math.log(2,3))

print(math.log2(16))

print(math.log10(10000))

print(math.pow(3,2))

print(math.sqrt(25))

print(math.sin(3.14/6))

print(math.cos(3.14/6))

print(math.tanh(3.14/6))

print(math.hypot(3,4))

print(math.radians(180))

print(math.gamma(4))

print(math.pi)

print(math.e)

print(math.isnan(15))
 
print(math.isinf(67647))
